$(NAME): ${o_s}
	@ar rc $(NAME) $(o_s)

.c.o:
	@${CC} ${flags} -c $< -o ${<:.c=.o}

bonus: ${o_s} ${o_s_b}
	@ar rc $(NAME) $(o_s) $(o_s_b)
clean:
	@rm -rf $(o_s) $(o_s_b)

fclean: clean
	@rm -rf $(NAME)

re: fclean all

.PHONY: all clean fclean re bonus